{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/libwebrtc/src",
                "${workspaceFolder}/webrtc-sys/include",
                "${workspaceFolder}/livekit-ffi/include",
                "/usr/include/c++/12",
                "/usr/include/x86_64-linux-gnu/c++/12",
                "${workspaceFolder}/livekit-ffi/include/**"
            ],
            "defines": [
                "WEBRTC_POSIX",
                "WEBRTC_LINUX",
                "RUST_CXX_NO_EXCEPTIONS"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-std=c++20",
                "-fPIC",
                "-Wall",
                "-Wextra",
                "-Wno-unused-parameter"
            ]
        }
    ],
    "version": 4
}